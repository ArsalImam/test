                android:layout_marginRight="@dimen/margin.small"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_7sdp"
                android:paddingTop="@dimen/_7sdp">



                <RatingBar
                    android:id="@+id/driverRb"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_26sdp"
                    android:clickable="false"
                    android:focusableInTouchMode="true"
                    android:isIndicator="true"
                    android:max="5"
                    android:numStars="5"
                    android:progressDrawable="@drawable/custom_rating_bar"
                    android:rating="0"
                    android:stepSize="0.1" />


                <com.bykea.pk.partner.widgets.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right|center_vertical"
                    android:paddingRight="@dimen/_4sdp"
                    android:text="پارٹنر کی طرف سے ریٹنگ"
                    android:textColor="@color/textColorPrimary"
                    android:textSize="@dimen/_12sdp"
                    app:fontName="jameel_noori_nastaleeq.ttf" />
            </LinearLayout>

