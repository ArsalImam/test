                        android:layout_height="@dimen/height.large"
                        android:layout_below="@+id/dotThreeIv"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:drawablePadding="@dimen/padding.normal"
                        android:gravity="left|center_vertical"
                        android:maxLines="1"
                        android:padding="@dimen/padding.tiny"
                        android:textColor="@color/secondaryColorDark"
                        android:textSize="@dimen/textsize.small.medium"
